{
  "compilerOptions": {
    "declaration": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "module": "commonjs",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "target": "ES2018"
  },
  "exclude": ["cdk.out", "**/__tests__"]
}
